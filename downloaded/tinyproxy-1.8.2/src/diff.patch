+/**
+ * If the access list has not been set up, create it.
+ */
+static int init_access_list(vector_t *access_list)
+{
+        if (!*access_list) {
+                *access_list = vector_create ();
+                if (!*access_list) {
+                        log_message (LOG_ERR,
+                                     "Unable to allocate memory for access list");
+                        return -1;
+                }
+        }
+
+        return 0;
+}
